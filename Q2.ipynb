    "import numpy as np\n",
    "\n",
    "def reverse(string):\n",
    "    result = [s[::-1] for s in strings if len(s) >= 5]\n",
    "    return np.array(result)\n",
    "\n",
    "strings = [\"Himanshu\", \"Yash\", \"Om\", \"Dhruv\", \"Rambow\", \"Shivanshu\"]\n",
    "reversed = reverse(string)\n",
    "print(\"Reversed string:\", reversed)\n"
